[qssClass="CheckBox"] {
    color: black;
    /* font: 14px --FontFamilies; */
    spacing: 8px;
    min-width: 28px;
    min-height: 22px;
    outline: none;
    margin-left: 1px;
}

[qssClass="CheckBox"]::indicator {
    width: 18px;
    height: 18px;
    border-radius: 5px;
    border: 1px solid transparent;
    background-color: transparent;
}

[qssClass="CheckBox"]:disabled {
    color: rgba(0, 0, 0, 0.36);
}
